{
    "name": "Flask DevContainer",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "forwardPorts": [5000],
    "postCreateCommand": "pip install -r requirements.txt",
    "settings": {
        "terminal.integrated.defaultProfile.linux": "/bin/bash"
    },
    "extensions": [
        "ms-python.python"
    ]
}
